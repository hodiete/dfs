{
    "name": "acquia/acsfdev",
    "description": "WebNY",
    "license": "proprietary",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "acquia/blt": "8.6.11",
        "behat/behat": "^3.1",
        "behat/mink": "~1.7",
        "behat/mink-browserkit-driver": "*",
        "behat/mink-selenium2-driver": "^1.3.1",
        "cweagans/composer-patches": "^1.6.0",
        "drupal-composer/drupal-scaffold": "^2.1.0",
        "drupal/acquia_connector": "^1.5.0",
        "drupal/acsf": "^1.37.0",
        "drupal/admin_toolbar": "1.19",
        "drupal/adminimal_theme": "^1.3",
        "drupal/advagg": "2.0-rc2",
        "drupal/allowed_formats": "1.1.0",
        "drupal/amber_alert_module": "dev-master",
        "drupal/better_exposed_filters": "3.0-alpha3",
        "drupal/blazy": "1.0-rc2",
        "drupal/coder": "^8.2.8",
        "drupal/cog": "^1.0.0",
        "drupal/console": "^1.0.0-rc13",
        "drupal/core": "8.3.7",
        "drupal/crop": "1.2",
        "drupal/csv_serialization": "1.0",
        "drupal/devel": "^1.0.0-alpha1",
        "drupal/diff": "1.0-alpha2",
        "drupal/dropzonejs": "1.0-alpha7",
        "drupal/drupal-extension": "^3.2",
        "drupal/editor_advanced_link": "1.4",
        "drupal/embed": "dev-1.x",
        "drupal/entity": "1.0-alpha4",
        "drupal/entity_browser": "1.1",
        "drupal/entity_embed": "dev-1.x",
        "drupal/entity_reference_revisions": "1.3",
        "drupal/facets": "^1.0",
        "drupal/features": "3.5.0",
        "drupal/field_group": "1.0-rc6",
        "drupal/focal_point": "1.0-beta4",
        "drupal/google_analytics": "^2.1.0",
        "drupal/honeypot": "1.27",
        "drupal/inline_entity_form": "1.0-beta1",
        "drupal/libraries": "dev-3.x",
        "drupal/linked_field": "dev-1.x",
        "drupal/login_security": "1.3",
        "drupal/masquerade": "1.0-beta1",
        "drupal/media_entity": "1.5",
        "drupal/media_entity_embeddable_video": "1.0-beta2",
        "drupal/media_entity_image": "1.2",
        "drupal/media_expire": "1.0-alpha2",
        "drupal/memcache": "2.0-alpha4",
        "drupal/metatag": "1.2",
        "drupal/paragraphs": "1.1",
        "drupal/password_policy": "3.0-alpha3",
        "drupal/pathauto": "1.0",
        "drupal/redirect": "1.0-beta1",
        "drupal/rename_admin_paths": "1.1",
        "drupal/search_api": "^1.0",
        "drupal/search_api_solr": "^1.0-rc3",
        "drupal/seckit": "^1.0.0-alpha2",
        "drupal/security_review": "*",
        "drupal/services": "4.0-beta4",
        "drupal/shield": "^1.0.0",
        "drupal/simple_sitemap": "2.9",
        "drupal/sitemap": "1.3",
        "drupal/smart_trim": "1.0",
        "drupal/token": "1.0",
        "drupal/twig_tweak": "^1.8",
        "drupal/video_embed_field": "1.5",
        "drupal/webform": "5.0.0-beta11",
        "drupal/workbench_moderation": "1.2.0",
        "drush/drush": "^8.1.8",
        "geerlingguy/drupal-vm": "^4.0",
        "grasmash/yaml-cli": "^0.3",
        "guzzlehttp/guzzle": "^6.0@dev",
        "hirak/prestissimo": "^0.3.4",
        "jarnaiz/behat-junit-formatter": "^1.3.2",
        "league/csv": "*",
        "phpunit/phpunit": "^4.8",
        "roave/security-advisories": "dev-master",
        "se/selenium-server-standalone": "^2.53",
        "squizlabs/php_codesniffer": "2.*",
        "symfony/console": "^2.8.11",
        "drupal/video_embed_brightcove": "1.x-dev"
    },
    "scripts": {
        "blt-alias": "blt install-alias -Dcreate_alias=true",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock",
            "@composer clearcache",
            "@composer install"
        ],
        "install-phantomjs": [
            "rm -rf vendor/bin/phantomjs",
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-drupal-scaffold-cmd": [
            "cd docroot && ../scripts/wny/update-htaccess.sh ../patches/htaccess-redirects.patch && cd -",
            "cd docroot && ../scripts/wny/update-htaccess.sh ../patches/htaccess-ssl.patch && cd -",
            "rm -f docroot/.htaccess.orig"
        ],
        "post-install-cmd": [
            "cd docroot/profiles/custom/webny/libraries && npm install && rsync -a node_modules/ ./ && rm -rf node_modules/*"
        ],
        "post-update-cmd": [
            "cd docroot/profiles/custom/webny/libraries && npm install && rsync -a node_modules/ ./ && rm -rf node_modules/*"
        ]
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "custom": {
            "type": "git",
            "url": "git@github.com:ny/amber_alert_module.git"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "extra": {
        "blt": {
            "update": true
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            },
            "excludes": [
                "sites/development.services.yml"
            ]
        },
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "Ignore front end vendor folders to improve directory search performance": "https://www.drupal.org/files/issues/ignore_front_end_vendor-2329453-116.patch",
                "Check for the presence of image file array before using it in a function call": "https://www.drupal.org/files/issues/2745491-15.patch",
                "Turn Views exposed filters for dates into date fields": "https://www.drupal.org/files/issues/views_exposed_filter-2710793-8.patch"
            },
            "drupal/adminimal_theme": {
                "Incompatible with admin toolbar": "https://www.drupal.org/files/issues/incompatible_with_admin-2635456-4_0.patch"
            },
            "drupal/dropzonejs": {
                "Add missing EB widget schema": "https://www.drupal.org/files/issues/add_missing_eb_widget-2813143-2.patch"
            },
            "drupal/linked_field": {
                "Fix breaking of picture markup": "https://www.drupal.org/files/issues/linked_field_breaks-2319165-7.patch"
            },
             "drupal/media_entity": {
                "class Media extends ContentEntityBase implements MediaInterface": "https://www.drupal.org/files/issues/2749695_14_media_entity.patch"
            },
            "drupal/media_entity_image": {
                "class Image extends MediaTypeBase": "https://www.drupal.org/files/issues/2749695_14_media_entity_image.patch"
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/profiles/custom/webny/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/profiles/custom/webny/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/profiles/custom/webny/libraries/{$name}": [
                "type:drupal-library"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        }
    },
    "require-dev": {
        "behatch/contexts": "^2.6"
    }
}
