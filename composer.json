{
    "name": "acquia/acsfdev",
    "description": "WebNY",
    "license": "proprietary",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "acquia/blt": "8.6.8",
        "cweagans/composer-patches": "^1.6.0",
        "drupal/acquia_connector": "^1.5.0",
        "drupal/acsf": "^1.34.0",
        "drupal/adminimal_theme": "^1.3",
        "drupal/cog": "^1.0.0",
        "drupal/admin_toolbar": "1.18",
        "drupal/advagg": "2.0-rc2",
        "drupal/better_exposed_filters": "3.0-alpha1",
        "drupal/blazy": "1.0-beta6",
        "drupal/core": "8.2.5",
        "drupal/contact_storage": "1.0-beta8",
        "drupal/crop": "1.0",
        "drupal/features": "^3.2.0",
        "drupal/diff": "1.0-alpha2",
        "drupal/dropzonejs": "1.0-alpha3",
        "drupal/embed": "dev-1.x",
        "drupal/entity": "1.0-alpha4",
        "drupal/entity_browser": "1.0-alpha9",
        "drupal/entity_embed": "dev-1.x",
        "drupal/entity_reference_revisions": "1.0",
        "drupal/inline_entity_form": "1.0-beta1",
        "drupal/memcache": "2.x-dev",
        "drupal/focal_point": "1.0-beta3",
        "drupal/field_group": "1.0-rc6",
        "drupal/google_analytics": "2.1.0",
        "drupal/honeypot": "1.23",
        "drupal/libraries": "dev-3.x",
        "drupal/linked_field": "dev-1.x",
        "drupal/seckit": "^1.0.0-alpha2",
        "drupal/login_security": "1.2",
        "drupal/masquerade": "1.0-beta1",
        "drupal/media_entity": "1.5",
        "drupal/media_entity_embeddable_video": "1.0-beta2",
        "drupal/media_entity_image": "1.2",
        "drupal/media_expire": "1.0-alpha2",
        "drupal/metatag": "1.0.0-beta10",
        "drupal/pathauto": "1.0-beta1",
        "drupal/paragraphs": "1.0",
        "drupal/password_policy": "3.0-alpha2",
        "drupal/redirect": "1.0-alpha1",
        "drupal/security_review": "*",
        "drupal/shield": "^1.0.0",
        "drupal/services": "4.0-beta2",
        "drupal/sitemap": "1.2",
        "drupal/simple_sitemap": "2.7",
        "drupal/smart_trim": "1.0",
        "drupal/yamlform": "1.x-dev",
        "guzzlehttp/guzzle": "^6.0@dev",
        "drush/drush": "^8.1.8",
        "roave/security-advisories": "dev-master",
        "drupal/token": "1.0-rc1",
        "drupal/video_embed_field": "1.3",
        "drupal/workbench_moderation": "1.1.0",
        "league/csv": "*",
        "drupal/csv_serialization": "1.0",
        "hirak/prestissimo": "^0.3.4",
        "drupal/console": "^1.0.0-rc13",
        "acquia/lightning": "2.0.1",
        "drupal/amber_alert_module": "dev-master"
    },
    "require-dev": {
        "behat/behat": "^3.1",
        "behat/mink": "~1.7",
        "behat/mink-selenium2-driver": "^1.3.1",
        "drupal/devel": "^1.0.0-alpha1",
        "behat/mink-browserkit-driver": "*",
        "drupal/drupal-extension": "^3.2",
        "drupal-composer/drupal-scaffold": "^2.1.0",
        "jarnaiz/behat-junit-formatter": "^1.3.2",
        "drupal/coder": "^8.2.8",
        "phpunit/phpunit": "~5.4",
        "se/selenium-server-standalone": "^2.53",
        "squizlabs/php_codesniffer": "2.*",
        "geerlingguy/drupal-vm": "~4.0",
        "symfony/console": "^2.8.11",
        "grasmash/yaml-cli": "^0.3"
    },
    "scripts": {
        "blt-alias": "blt install-alias -Dcreate_alias=true",
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "nuke": [
            "rm -rf vendor composer.lock",
            "@composer clearcache",
            "@composer install"
        ],
        "install-phantomjs": [
            "rm -rf vendor/bin/phantomjs",
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    },
    "repositories": {
    "drupal": {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
    },
        "custom": {
            "type": "vcs",
            "url": "git@github.com:ny/amber_alert_module.git"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "extra": {
        "blt": {
            "update": true
        },
        "drupal-scaffold": {
            "initial": {
                "sites/default/default.services.yml": "sites/default/services.yml",
                "sites/default/default.settings.php": "sites/default/settings.php"
            },
            "excludes": [
                "sites/development.services.yml"
            ]
        },
        "enable-patching": true,
        "patches": {
            "acquia/lightning": {
                "New LightningExtension subcontexts do not autoload": "https://www.drupal.org/files/issues/2836258-3-lightning-extension-autoload.patch"
            }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/profiles/custom/webny/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/webny/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        }
    }
}
