{
    "name": "acquia/acsfdev",
    "license": "proprietary",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "require": {
        "acquia/blt": "8.5.0-beta1",
        "nikic/php-parser": "2.0.1",
        "composer/installers": "^1.0.20",
        "cweagans/composer-patches": "^1.5.0",
        "drupal/acquia_connector": "8.1.*",
        "drupal/acsf": "8.1.32",
        "drupal/adminimal_theme": "8.1.2",
        "drupal/admin_toolbar": "8.1.17",
        "drupal/advagg": "8.2.0-rc2",
        "drupal/better_exposed_filters": "8.3.0-alpha1",
        "drupal/blazy": "8.1.0-beta4",
        "drupal/contact_storage": "8.1.0-beta8",
        "drupal/core": "8.2.3",
        "drupal/crop": "8.1.0",
        "drupal/diff": "8.1.0-alpha2",
        "drupal/devel": "8.*",
        "drupal/dropzonejs": "8.1.0-alpha2",
        "drupal/embed": "dev-8.x-1.x",
        "drupal/entity": "8.1.0-alpha3",
        "drupal/entity_browser": "8.1.0-alpha9",
        "drupal/entity_embed": "dev-8.x-1.x",
        "drupal/entity_reference_revisions": "8.1.0",
        "drupal/inline_entity_form": "8.1.0-alpha6",
        "drupal/features": "8.3.0-rc1",
        "drupal/focal_point": "8.1.0-beta3",
        "drupal/field_group": "dev-8.x-1.x#14173e352948b97a455b48586d3e022a864a38c6",
        "drupal/google_analytics": "8.2.1",
        "drupal/honeypot": "8.1.23",
        "drupal/libraries": "dev-8.x-3.x",
        "drupal/linked_field": "dev-8.x-1.x",
        "drupal/login_security": "8.1.2",
        "drupal/masquerade": "8.1.0-beta1",
        "drupal/media_entity": "8.1.5",
        "drupal/media_entity_embeddable_video": "8.1.0-beta2",
        "drupal/media_entity_image": "8.1.2",
        "drupal/media_expire": "8.1.0-alpha2",
        "drupal/memcache": "8.*",
        "drupal/metatag": "8.1.0-beta10",
        "drupal/pathauto": "8.1.0-beta1",
        "drupal/paragraphs": "8.1.0",
        "drupal/password_policy": "8.3.0-alpha2",
        "drupal/redirect": "8.1.0-alpha1",
        "drupal/security_review": "8.*",
        "drupal/services": "8.4.0-beta2",
        "drupal/sitemap": "8.1.2",
        "drupal/simple_sitemap": "8.2.6",
        "drupal/smart_trim": "8.1.0",
        "drupal/yamlform": "8.1.0-beta25",
        "guzzlehttp/guzzle": "^6.0@dev",
        "roave/security-advisories": "dev-master",
        "drupal/token": "8.1.0-beta2",
        "drupal/video_embed_field": "8.1.3",
        "drupal/workbench_moderation": "8.1.1",
        "drupal-composer/drupal-security-advisories": "8.0.x-dev",
        "league/csv": "*",
        "drupal/csv_serialization": "8.1.0",
        "hirak/prestissimo": "^0.3.4",
        "drupal/console": "1.0-beta5"
    },
    "require-dev": {
        "behat/behat": "^3.0",
        "behat/mink": "~1.7",
        "behat/mink-selenium2-driver": "^1.3.1",
        "behat/mink-browserkit-driver": "*",
        "drupal/drupal-extension": "^3.2",
        "drush/drush": "8.1.5",
        "jarnaiz/behat-junit-formatter": "^1.2",
        "drupal/coder": "~8.2.9",
        "phpunit/phpunit": "~5.4",
        "se/selenium-server-standalone": "^2.53",
        "squizlabs/php_codesniffer": "2.*",
        "phing/phing": "2.*"
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\PHPUnit\\": "tests/phpunit/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/profiles/custom/webny/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/webny/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "Ignore front end vendor folders to improve directory search performance": "https://www.drupal.org/files/issues/ignore_front_end_vendor-2329453-116.patch",
                "Twig debug output does not display all suggestions when an array of suggestions is passed to theme": "https://www.drupal.org/files/issues/twig_debug_output_does-2118743-89.patch",
                "Turn Views exposed filters for dates into date fields": "https://www.drupal.org/files/issues/views_exposed_filter-2710793-8.patch",
                "Addresses undefined index error": "https://www.drupal.org/files/issues/2811887-2-exposed-date-restore-type-ealier.patch"
            },
            "drupal/adminimal_theme": {
                "Incompatible with admin toolbar": "https://www.drupal.org/files/issues/incompatible_with_admin-2635456-4_0.patch"
            },
            "drupal/metatag": {
                "Fatal error after updating to 8.1.0-beta10": "https://www.drupal.org/files/issues/metatag-deactivate_drupalconsole_services.patch"
            },
            "drupal/dropzonejs": {
                "Add missing EB widget schema": "https://www.drupal.org/files/issues/add_missing_eb_widget-2813143-2.patch"
            },
            "drupal/entity_browser": {
                "Refactory \\Drupal\\entity_browser\\Plugin\\Field\\FieldWidget\\EntityReferenceBrowserWidget::formElement to allow more advanced usecases": "https://www.drupal.org/files/issues/2802969-5.patch",
                "Entity Browser widget loses selected images in inline entity form": "https://www.drupal.org/files/issues/2764889_31.patch",
                "Incomplete schema for multi_step_display": "https://www.drupal.org/files/issues/incomplete_schema_for-2812901-2.patch"
            },
            "drupal/focal_point": {
                "Add image effect schema": "https://www.drupal.org/files/issues/add_image_effect_schema-2812835-3.patch"
            },
            "drupal/media_entity": {
                "Add field formatter schema": "https://www.drupal.org/files/issues/add_field_formatter-2812867-3.patch"
            },
            "drupal/linked_field": {
                "Fix breaking of picture markup": "https://www.drupal.org/files/issues/linked_field_breaks-2319165-7.patch"
            }
        }
    },
    "scripts": {
    "blt-alias": "blt install-alias -Dcreate_alias=true",
    "nuke": [
        "rm -rf vendor composer.lock",
        "@composer clearcache",
        "@composer install"
    ]
    }
}
