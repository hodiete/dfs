{#
    USED SPECIFICALLY FOR THE GLOBAL NAVIGATION BLOCK IN THE HEADER REGION
    -- See region--header.twig.html for parent definition template
#}
{% if headerauto == 1 %}
    {% if unavsetting == '1' %}
        <nav id="webny-global-header" class="webny-global-header {{ headerformat }} alt-unav" aria-label="primary-site" >
    {% else %}
        <nav id="webny-global-header" class="webny-global-header {{ headerformat }}" aria-label="primary-site" >
    {% endif %}
        <div class="nav-toggle">
            <a href="#" role="button" id="webny-menu-control">Navigation menu</a>
        </div>
    {% if agencylogo is not null and agencylogo is not empty %}
        <div class="webny-global-logo"><a href="/"><img src="{{ agencylogo }}" alt="{{ agencyname|raw }}" /></a></div>
    {% else %}
        <h1><a href="/">{{ agencyname|raw }}</a></h1>
    {% endif %}
    {{ globalNavBlock(headermenu) }}
    </nav>
{% endif %}