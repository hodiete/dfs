<div class="announcement-wrap">

    <div class="webny-announcement-frame-inner">
        {%  if((paragraph.field_webny_announcement_title.value is not empty and paragraph.field_webny_announcement_title.value is not null) or (paragraph.field_webny_announcement_headline.value is not empty and paragraph.field_webny_announcement_headline.value is not null)) %}
            <h2>
                    <span class="frame-title webny-frametitle">
                        {{ paragraph.field_webny_announcement_title.value }}
                    </span>
                    <br/>
                    <span class="frame-headline webny-headline">
                         {{ paragraph.field_webny_announcement_headlne.value }}
                    </span>
            </h2>
        {% else %}
        {% endif %}

    </div>

    {{ content|without('field_webny_announcement_title', 'field_webny_announcement_headlne') }}
</div>
