{#
/**
 * @file
 * Theme override to display a node.
 * OVERRIDE the news display for quick links card.
 */
#}
<article{{ attributes.addClass('webny-quick-links') }}>

    <div{{ content_attributes }}>
        <div class="webny-quick-links-image"><a href="{{ url }}">{{ content.field_webny_news_image }}</a></div>
        <div class="webny-quick-links-card-text">
            <div class="webny-quick-links-title"><a href="{{ url }}">{{ content.field_webny_news_short_title }}</a></div>
            <div class="webny-quick-links-description">{{ content.field_webny_news_description }}</div>
        </div>
    </div>

</article>
