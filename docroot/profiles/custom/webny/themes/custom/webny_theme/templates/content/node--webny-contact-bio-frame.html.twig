{#
/**
 * @file
 * Theme override to display a node.
 *
 * CREATED BY WEBNY -- iojonep, lmurdock, mcase
 * Override theme for bio frames
 * Available variables:
 * https://api.drupal.org/api/drupal/core!modules!node!templates!node.html.twig/8.2.x 
#}

{%
  set docclasses = [
    node.bundle ~ "_bioframe"|clean_class,
  ]
%}

{%
    set bioimg = file_url(node.field_webny_contact_image.entity.image.entity.uri.value) 
%}

{# DEBUG VIA KINT / DEVEL #}

<article{{ attributes.addClass(docclasses) }}>
        {% for i in content %}  
            {% if 'bio-frame-image' in i %}
               {{ i }}
            {% else %}
               {{ i }} 
            {% endif %}
        {% endfor %}
</article>
