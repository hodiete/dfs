{%
  set docclasses = [
    node.bundle ~ "_bioframe"|clean_class,
  ]
%}

{# DEBUG VIA KINT / DEVEL #}

<article{{ attributes.addClass(docclasses) }}>
        {% for i in content %} 
               {{ i }} 
        {% endfor %}
</article>
